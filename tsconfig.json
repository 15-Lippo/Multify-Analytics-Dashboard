{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Navbars/VerticalNarbar/VerticalNavToken/index.tsx",
    "components/DefiInterfaces/SwapInterface/index.tsx",
    "pages/swapinterface.tsx",
    "components/DefiInterfaces/WalletInterface/index.js",
    "components/DefiInterfaces/WalletInterface/SelectButton.tsx",
    "components/Tables/Pools/index.tsx",
    "components/Buttons/ChainsButton/index.js",
    "components/Buttons/ChainsButton/index.js",
    "components/Categroy/DexSelectBtn/index.js",
    "components/Category/SupportedDexes/index.js",
    "components/Category/Overview/index.js",
    "components/Category/Tokens/index.js",
    "components/Category/Tokens/TokenTable/index.js",
    "components/Category/Pools/index.js",
    "components/Category/Pools/PoolTable/index.js"
  ],
  "exclude": ["node_modules"]
}
